    {
      "path": "dist/redux-persist-x.esm.js",
      "limit": "10 KB"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/masoud-shahpoori/redux-persist-x"
  },
  "homepage": "https://github.com/masoud-shahpoori/redux-persist-x#readme",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "husky": "^9.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "size-limit": "^11.1.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3"
  }
}
